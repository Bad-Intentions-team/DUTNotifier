<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">
    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.dutnotifier.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_YourClass"
        android:name="com.example.dutnotifier.ui.your_class.YourClassFragment"
        android:label="@string/title_YourClass"
        tools:layout="@layout/fragment_your_class" />
    <fragment
        android:id="@+id/navigation_AboutUs"
        android:name="com.example.dutnotifier.ui.aboutUs.AboutUsFragment"
        android:label="@string/title_AboutUs"
        tools:layout="@layout/fragment_aboutus" />
